<Window x:Class="ProjektGik2f7.removeWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjektGik2f7"
        mc:Ignorable="d"
        Title="Remove Game" Height="300" Width="400">
    <Grid Background="#FFFF8000">
        <TextBox x:Name="idInput" HorizontalAlignment="Center" Margin="0,90,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,53,0,0" Text="Type in the specific ID of the game you want to remove" TextWrapping="Wrap" VerticalAlignment="Center" Height="32" Width="151" FontWeight="Bold"/>
        <Button x:Name="btnIdRmv" Content="Confirm" HorizontalAlignment="Left" Margin="276,200,0,0" VerticalAlignment="Top" Click="btnIdRmv_Click"/>
        <Button x:Name="btnCancel" Content="Cancel" HorizontalAlignment="Left" Margin="80,200,0,0" VerticalAlignment="Top" Click="btnCancel_Click"/>
    </Grid>
</Window>
